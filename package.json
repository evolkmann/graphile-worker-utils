  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "demo:db:reset": "yarn demo:db:down && yarn demo:db:up",
    "demo:db:down": "docker-compose -f demo/docker-compose.yml -p gwu_demo down -v",
    "demo:db:up": "docker-compose -f demo/docker-compose.yml -p gwu_demo up -d",
    "demo:worker:install-schema": "node -r dotenv/config ./node_modules/.bin/graphile-worker --schema-only"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "dotenv": "16.3.1",
    "graphile-worker": "0.14.0-rc.0",
    "jest": "29.6.2",
    "pg": "8.11.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "graphile-worker": "^0.14.0-rc.0",
    "pg": ">=6.5 <9"
  }
}
